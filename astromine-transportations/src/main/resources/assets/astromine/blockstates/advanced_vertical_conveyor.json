{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "front": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_extension"
      }
    },
    {
      "when": {
        "facing": "east",
        "front": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_extension",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "front": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_extension",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "front": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_extension",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "conveyor": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_connector"
      }
    },
    {
      "when": {
        "facing": "east",
        "conveyor": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_connector",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "conveyor": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_connector",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "conveyor": "true"
      },
      "apply": {
        "model": "astromine:block/advanced_vertical_conveyor_connector",
        "y": 270
      }
    }
  ]
}